<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <script>
$(document).ready(function(){
    $("#show-all").click(function(){
		$("li.all").show();
        $("li.all").show();
    });
    $("#show-art").click(function(){
		$("li.art").hide();
        $("li.art").show();
        $("li.biz").hide();
        $("li.dance").hide();
    });
    $("#show-biz").click(function(){
        $("li.art").hide();
		$("li.biz").hide();
        $("li.biz").show();
        $("li.dance").hide();
    });
    $("#show-dance").click(function(){
        $("li.art").hide();
        $("li.biz").hide();
		$("li.dance").hide();
        $("li.dance").show();
    });
});
</script>
